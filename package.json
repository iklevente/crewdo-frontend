{
  "name": "crewdo-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "rimraf dist && rspack serve",
    "build": "rimraf dist && rspack build",
    "build:prod": "npm run build",
    "lint": "npm run tsc:check && npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "eslint \"src/**/*.{js,ts,tsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "tsc:check": "tsc --noEmit",
    "security:check": "npm audit --registry=https://registry.npmjs.org --audit-level=critical",
    "security:fix": "npm audit fix --registry=https://registry.npmjs.org && rimraf ./node_modules && npm cache clear --force && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "eslint": "^9.35.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.4.13",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rspack/cli": "^1.5.3",
    "@rspack/core": "^1.5.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
