{
  "name": "crewdo-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "rimraf dist && rsbuild dev",
    "dev": "rimraf dist && rsbuild dev",
    "build": "rimraf dist && rsbuild build",
    "build:prod": "npm run build",
    "lint": "npm run tsc:check && npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "eslint \"src/**/*.{js,ts,tsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "tsc:check": "tsc --noEmit",
    "prune": "ts-prune",
    "tsr:check": "tsr src/index.tsx",
    "tsr:clean": "tsr --write src/index.tsx",
    "tsr:clean:recursive": "tsr --recursive --write src/index.tsx",
    "cleanup": "npm run lint && npm run tsr:clean",
    "security:check": "npm audit --registry=https://registry.npmjs.org --audit-level=critical",
    "security:fix": "npm audit fix --registry=https://registry.npmjs.org && rimraf ./node_modules && npm cache clear --force && npm install",
    "prepare": "husky",
    "copy-api-client": "./copy-api-client.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "^2.2.9",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.0",
    "@mui/material": "^7.3.4",
    "@mui/x-data-grid": "^8.14.1",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "emoji-picker-element": "^1.27.0",
    "eslint": "^9.35.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.3.6",
    "ts-prune": "^0.10.3",
    "tsr": "^1.3.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.4.13",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rspack/cli": "^1.5.3",
    "@rspack/core": "^1.5.3",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
